import matplotlib.pyplot as plt

population_ages=[10,12,20,22,23,25,29,50,70,90,80,44,56,58,60,78,55,29,30,45]
bin = [0,20,40,60,70,80,90]
#ids = [x for x in range(len(population_ages))]

plt.hist(population_ages, bin, histtype='bar',rwidth=0.8)

plt.xlabel('x')
plt.ylabel('y')
plt.title('Bar')

plt.show()
